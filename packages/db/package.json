{
  "name": "@nephix/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc -p tsconfig.json",
    "test": "vitest run --passWithNoTests",
    "db:generate": "pnpm exec prisma generate",
    "db:migrate": "pnpm exec prisma migrate deploy",
    "db:seed": "pnpm db:generate && pnpm exec tsx src/seed.ts"
  },
  "dependencies": {
    "@nephix/contracts": "workspace:*",
    "@nephix/domain": "workspace:*",
    "@prisma/client": "6.2.1",
    "zod": "3.24.1"
  },
  "devDependencies": {
    "prisma": "6.2.1",
    "tsx": "4.19.2"
  },
  "prisma": {
    "seed": "tsx src/seed.ts"
  }
}
